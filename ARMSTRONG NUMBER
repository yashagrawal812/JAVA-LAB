
public class ArmstrongNumber {
public static void main(String[] args)
{
	int num=370,sum=0,num2,num1;
	num1=num;
			while(num1!=0)
			{
				num2=num1%10;
				num1=num1/10;
				sum+=(num2*num2*num2);
			}
	if(num==sum)
	{
		System.out.println("The number is Armstrong");
	}
	else
		System.out.println("The number is not Armstrong");
}
}
