//program to find the sum of numbers using command line argument
public class sum_of_cmdargs {

	public static void main(String[] args) {
		int i,sum=0;
		System.out.println("Enter the Numbers for the sum");
		for(i=0;i<args.length;i++)
		{sum+=Integer.parseInt(args[i]);
	    }
		for(i=0;i<args.length;i++)
		{
			System.out.println("Element is : "+args[i]);
		}
		System.out.println("Sum of elemnts : "+sum);

	}

}
